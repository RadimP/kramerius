<testSuite testType="mods" model="monograph">

    <test name="Jaro 2012">
        <desc>Jaro roku</desc>
        <in>
            <originInfo>
                <dateIssued>Jaro 2012</dateIssued>
            </originInfo>
        </in>
        <out>
            <field name="date.max">2012-06-20T23:59:59.999Z</field>
            <field name="date.min">2012-03-21T00:00:00.001Z</field>
            <field name="date.str">Jaro 2012</field>
            <field name="date_range_end.day">20</field>
            <field name="date_range_end.month">6</field>
            <field name="date_range_end.year">2012</field>
            <field name="date_range_start.day">21</field>
            <field name="date_range_start.month">3</field>
            <field name="date_range_start.year">2012</field>
        </out>
    </test>

